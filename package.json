{
  "name": "dreamlust-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "npm@9.8.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "start": "vite preview --port 3000 --host",
    "lint": "eslint . --max-warnings=50",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ci": "playwright install --with-deps && playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "lhci": "lhci autorun",
    "lighthouse": "lhci autorun",
    "lighthouse:local": "lhci autorun --collect.startServerCommand='npm run preview'",
    "lighthouse:ci": "lhci autorun",
    "lighthouse:build": "npm run build && npm run lhci",
    "test:preview": "node test-preview.js",
    "clean": "powershell -ExecutionPolicy Bypass -File ./CLEAN_BUILD.ps1",
    "clean:build": "powershell -ExecutionPolicy Bypass -Command \"if (Test-Path dist) { Remove-Item -Recurse -Force dist }; if (Test-Path node_modules/.vite) { Remove-Item -Recurse -Force node_modules/.vite }; npm run build\"",
    "kill:port": "powershell -ExecutionPolicy Bypass -File ./kill-port.ps1",
    "preview:force": "powershell -ExecutionPolicy Bypass -File ./kill-port.ps1"
  },
